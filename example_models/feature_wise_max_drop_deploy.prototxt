  type: "ReLU"
}
layer {
  bottom: "conv3_3_maxdrop"
  top: "pool3"
  name: "pool3"
  type: "Pooling"
  pooling_param {
    pool: MAX
    kernel_size: 2
    stride: 2
  }
}
layer {
  bottom: "pool3"
  top: "conv4_1"
  name: "conv4_1"
   type: "Convolution"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "conv4_1_maxdrop"
  type: "MaxDrop"
  bottom: "conv4_1"
  top: "conv4_1_maxdrop"
  max_drop_param {
    prob: 0.2
    kernel_h: 1
    kernel_w: 1
    dim_option: 2
  }
}
layer {
  bottom: "conv4_1_maxdrop"
  top: "conv4_1_maxdrop"
  name: "relu4_1"
  type: "ReLU"
}
layer {
  bottom: "conv4_1_maxdrop"
  top: "conv4_2"
  name: "conv4_2"
  type: "Convolution"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "conv4_2_maxdrop"
  type: "MaxDrop"
  bottom: "conv4_2"
  top: "conv4_2_maxdrop"
  max_drop_param {
    prob: 0.2
    kernel_h: 1
    kernel_w: 1
    dim_option: 2
  }
}
layer {
  bottom: "conv4_2_maxdrop"
  top: "conv4_2_maxdrop"
  name: "relu4_2"
  type: "ReLU"
}
layer {
  bottom: "conv4_2_maxdrop"
  top: "conv4_3"
  name: "conv4_3"
  type: "Convolution"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  convolution_param {
    num_output: 512
    pad: 1
    kernel_size: 3
    weight_filler {
      type: "xavier"
