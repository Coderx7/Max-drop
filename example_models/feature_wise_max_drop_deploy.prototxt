  pooling_param {
    pool: AVE
    kernel_size: 4
    stride: 1
  }
}
layer {
  bottom: "pool5"
  top: "pool5_drop"
  name: "drop_ip"
  type: "Dropout"
  dropout_param {
    dropout_ratio: 0.5
  }
}
layer {
  bottom: "pool5_drop"
  top: "ip"
  name: "ip"
  type: "InnerProduct"
  param {
    lr_mult: 1
  }
  param {
    lr_mult: 2
  }
  inner_product_param {
    num_output: 10
    weight_filler {
      type: "xavier"
    }
    bias_filler {
      type: "constant"
    }
  }
}
layer {
  name: "prob"
  type: "Softmax"
  bottom: "ip"
  top: "prob"
}


